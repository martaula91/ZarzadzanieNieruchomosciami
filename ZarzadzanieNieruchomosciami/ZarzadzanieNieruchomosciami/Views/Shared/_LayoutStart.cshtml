
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" />
</head>
<body>
    <header>

        <div style="overflow: hidden;">

            @Html.Partial("_LoginPartial")

            <a id="indexHeaderLogo" href="@Url.Content("~/")">
                <img src="@Url.Content("~/Content/Images/logo.png")" class="center" />
            </a>

            @*<a class="manageLink" href="@Url.Action("Register", "Account")">
                Dodaj konto
            </a>*@

        </div>
    </header>



    <div id="ListaKategoriZawartosc">
        @Html.Action("KategorieMenu")   <!--Html.MvcSiteMap().SiteMapPath()-->

        <div id="KategorieListPrawaSekcja">
            @*<div id="Breadcrumb">
                    <img src="../Content/Images/dommaly.png" />
                    <a href="link">
                        @Html.MvcSiteMap().SiteMapPath()
                    </a>

                </div>*@

            <div>
                @RenderBody()
            </div>

        </div>
    </div>





    <footer>
        <nav>
            <ul>
                <li><a href="@Url.Action("StronyStatyczne","Home", new { nazwa = "Onas" })" style="">O nas</a></li>
                <li><a href="@Url.Action("StronyStatyczne","Home", new { nazwa = "Spolecznosc" })" style="">Spolecznosc</a></li>
                <li><a href="@Url.Action("StronyStatyczne","Home", new { nazwa = "Opinie" })" style="">Opinie</a></li>
                <li><a href="@Url.Action("StronyStatyczne","Home", new { nazwa = "Pomoc" })" style="">Pomoc</a></li>
                <li><a href="@Url.Action("StronyStatyczne","Home", new { nazwa = "Regulamin" })" style="">Regulamin</a></li>
                <li><a href="@Url.Action("StronyStatyczne","Home", new { nazwa = "Kontakt" })" style="">Kontakt</a></li>
            </ul>
        </nav>
        <div id="footerLogo">
        </div>
        <div id="footerCopyright">
            @*Copyright &copy; 2018  MZ*@
        </div>
    </footer>

    @RenderSection("Scripts", required: false)
</body>
</html>